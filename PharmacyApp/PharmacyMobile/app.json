    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4288f79c-61cf-4bb0-ad21-171a2046f045"
      }
    },
    "owner": "anass-nb1",
    "plugins": [
      "expo-localization"
    ]
  }
}
